@media (max-width: 768px) {
    .sidebar{
  	    width:220px;+
	  	position: absolute;
	  	top:70px;
	  	margin: 0;
	  	padding: 0;
	  	background-color: #fff;
	  	z-index: 1000;
	  	box-shadow: 2px 1px 2px 0 rgba(0,0,0,0.15);
	  	transform:translateX(-220px);
