.nav-tabs-custom {
	background-color:#f5f5f5;
	border-bottom: 1px solid #4c8fbd;
}

.nav-tabs>li.active>a, .nav-tabs>li.active>a:focus, .nav-tabs>li.active>a:hover{
	border-radius:0;
	border-top:2px solid #4C8FBD;
	color:#4C8FBD;
	font-weight:bold;
    border-right: 1px solid #4c8fbd;
    border-left: 1px solid #4c8fbd;
}
.nav-tabs-custom>li{width:30%; text-align:center;}
.nav-tabs-custom>li>a{color:#666;font-size:18px;}
.nav-tabs-custom>.tab-content {
}

.cos-tabs>li {
	width: 25%;
}

.nav-tabs-custom>.nav-tabs>li {
	text-align: center;
}

.nav-tabs-custom>.nav-tabs>li>a,
.nav-tabs-custom>.nav-tabs>li>a:hover {
	background: transparent;
	margin: 0;
}

.nav-tabs-custom>.cos-tabs>li.active>a,
.nav-tabs-custom>.nav-tabs>li.active>a:hover {
	border-top-color: transparent;
	border-left-color: #ddd;
	border-right-color: #ddd;
}

.nav-tabs-custom>.nav-tabs>li.active>a,
.nav-tabs-custom>.nav-tabs>li.active:hover>a {
	background-color: #fff;
	color: #444;
}

.nav-tabs-custom>.nav-tabs>li>a,
.nav-tabs-custom>.nav-tabs>li>a:hover {
	background: transparent;
	margin: 0;
}

.nav-tabs-custom>.nav-tabs>li>a:hover {
	border: 1px solid transparent;
}

.nav-tabs-custom>.nav-tabs>li:first-of-type>a,
.nav-tabs-custom>.nav-tabs>li:first-of-type>a:hover {
	border-left-color: transparent;
}

.nav-tabs-custom>.nav-tabs>li:last-of-type>a,
.nav-tabs-custom>.nav-tabs>li:last-of-type>a:hover {
	border-right-color: transparent;
}
.nav-tabs-header{box-shadow: none;}
.nav-tabs-header .nav-tabs{
	position: relative;
	top:-50px;
	border: none;
	margin-right: 3px;
}
.nav-tabs-header .nav-tabs li{
	padding: 0 10px;
}

.nav-tabs-header .nav-tabs li.active{
	background-color: #FFF;
	border-left: 1px solid #eee;
	border-right: 1px solid #eee;
}
.nav-tabs-header .nav-tabs li:first-of-type{
	border-left: none;
}
.nav-tabs-header .nav-tabs li:last-of-type{
	border-right: none;
}
.nav-tabs-header .nav-tabs li.active a{
	border: none;
}

.nav-tabs-custom2{
	border-bottom:none;
}
.nav-tabs-custom2>li>a{
	height:38px;
	color:#fff;
}
.nav-tabs-custom2>li.active>a{
	border:1px solid #4c8fbd !important;
	border-bottom:none !important;
	border-radius: 4px 4px 0 0 !important;
}
.nav-tabs-custom2>li>a:hover{
	color:#4C8FBD;
}
