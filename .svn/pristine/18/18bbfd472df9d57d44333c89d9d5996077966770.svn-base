.nav-list{
	list-style: none;
	padding: 0;
	border-bottom: 1px solid #e7e7e7 !important;
    border-top: 1px solid #FFFFFF !important;
}
.nav-list>li{
	position: relative;
}
.nav-list>li>a{
	display: block;
	padding:12px 15px;
	color: #fff;
    border-bottom: 1px solid #7cb4c5 !important;
    border-top: 1px solid #3d6875 !important;
    text-decoration: none;
    font-size:16px;
