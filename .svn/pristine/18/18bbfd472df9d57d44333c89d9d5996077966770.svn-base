	background-color: #1c6a84;
    /* color: #2B7DBC; */
	/* border-left:3px solid #3c8dbc; */
}
.nav-list>li>a i{font-size: 16px;}
.nav-list>li>a i.right-icon{font-size: 18px;}
.nav-list>li>ul.sub{
	padding-left: 0;
	background-color: #fff;
	display:none;
}
.nav-list>li>ul.sub>li{list-style: none;}
.nav-list>li>ul.sub>li>a{
	color: #257F9C;
	display: block;
	padding-left: 45px;
	line-height: 42px;
	text-decoration: none;
	font-size:16px;
	border-top:1px dotted #fff;
	background-color:#C0DDE1;
}
.nav-list>li>ul.sub>li:last-child>a{
	border-bottom:none;
}
.nav-list>li>ul.sub>li:first-child>a{
	border-top:none;
}
.nav-list>li>ul.sub>li>a:hover,.nav-list>li.active>ul.sub>li.sec-active>a{
	color: #257F9C;
	background-color:#accbd0;
}
